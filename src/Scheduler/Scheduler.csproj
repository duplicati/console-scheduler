<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <InvariantGlobalization>true</InvariantGlobalization>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="MassTransit.Quartz" Version="8.3.3"/>
        <PackageReference Include="MassTransit.SqlTransport.PostgreSQL" Version="8.3.3"/>
        <PackageReference Include="Npgsql" Version="9.0.2"/>
        <PackageReference Include="Quartz.Extensions.DependencyInjection" Version="3.13.1"/>
        <PackageReference Include="Quartz.Serialization.SystemTextJson" Version="3.13.1"/>
        <PackageReference Include="DuplicatiConsoleCommon" Version="1.0.0" />
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="quartz_tables_postgres.sql" />
    </ItemGroup>

</Project>
