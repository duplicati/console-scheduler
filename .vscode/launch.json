  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Scheduler",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dotnet: build",
      "program": "${workspaceFolder}/src/Scheduler/bin/Debug/net9.0/Scheduler.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Scheduler",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_URLS": "http://localhost:5007",
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
